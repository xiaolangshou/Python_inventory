	<script>

      (function(){
        var wait = document.getElementById('wait');
        var interval = setInterval(function(){
          var time = --wait.innerHTML;
          if(time <= 0) {
            location.href = '/';
            clearInterval(interval);
          };
        }, 1000);
      })();

        $(function(){

            //商汤之路切换
            $('.gyst-12>span').each(function(i){
                $(this).click(function(){
                    if($(this).hasClass('hover')){

                    }else{
                        $('.gyst-12>span').removeClass('hover');
                        $(this).addClass('hover');
                        $('.xshz-21>.xshz-24').hide();
                        $('.xshz-21>.xshz-24').eq(i).fadeIn()
                    }
                })
            })






//二级导航
            var headerHeight= $('header').outerHeight();
            var nrnavHeight = $('#nrnav-box').outerHeight();
            var nrnavTop = $('#nrnav-box').offset().top;
            var nav1;
            var nav2;
            var nav3;
            var nav4;

            $(window).scroll(function(){
                nav1 = Math.floor($('#nr-nav-con').find('.cp-title').eq(0).offset().top - headerHeight-nrnavHeight);
                nav2 = Math.floor($('#nr-nav-con').find('.cp-title').eq(1).offset().top - headerHeight-nrnavHeight);
                nav3 = Math.floor($('#nr-nav-con').find('.cp-title').eq(2).offset().top - headerHeight-nrnavHeight);
                nav4 = Math.floor($('#nr-nav-con').find('.cp-title').eq(3).offset().top - headerHeight-nrnavHeight);
                if($(window).scrollTop()<nav2){
                    $('#nr-nav>li>a').removeClass('hover');
                    $('#nr-nav>li>a').eq(0).addClass('hover');
                }else if($(window).scrollTop()>=nav2&&$(window).scrollTop()<nav3){
                    $('#nr-nav>li>a').removeClass('hover');
                    $('#nr-nav>li>a').eq(1).addClass('hover');
                }else if($(window).scrollTop()>=nav3&&$(window).scrollTop()<nav4){
                    $('#nr-nav>li>a').removeClass('hover');
                    $('#nr-nav>li>a').eq(2).addClass('hover');
                }else if($(window).scrollTop()>=nav4){
                    $('#nr-nav>li>a').removeClass('hover');
                    $('#nr-nav>li>a').eq(3).addClass('hover');
                }
                if($(window).scrollTop()>nrnavTop-headerHeight){
									$('header').addClass("headeractive")
                    $('#nrnav-box').addClass('active');
										$('#nrnav-box').css('top',0)
                    $('#nrnav-box-height').height(nrnavHeight);
                }else{
									$('header').removeClass('headeractive');
                    $('#nrnav-box').removeClass('active');
                    $('#nrnav-box').css('top',0)
                    $('#nrnav-box-height').height(0);
                };
            })
            var roll;
            $('#nr-nav>li').each(function(i){
                $(this).click(function(){
                    roll = Math.floor($('#nr-nav-con').find('.cp-title').eq(i).offset().top - headerHeight-nrnavHeight);
                    $('html , body').animate({scrollTop: roll});
                    return false;
                })
            })

        })

	</script>
